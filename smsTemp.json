    "vars": { "year": 2018, "make": "Tesla", "model": "Model 3", "city": "San Jose", "state": "CA", "price": "$49,950", "shortId": "a0ae1", "url": "https://onlyusedtesla.com/l/a0ae1" }
  },
  "KYC_APPROVED": {
    "envOverride": "SMS_TMPL_KYC_APPROVED",
    "default": "[OnlyUsedTesla] KYC approved. You can now publish listings and receive offers. Dashboard: {{url}}",
